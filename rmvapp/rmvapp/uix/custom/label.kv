
<CustomLabel>:
    halign: "left"
    valign: "middle"
    text_size: self.size
    font_size: self.height * 0.5

<GroupParameter>:
    canvas.before:
        Color:
            rgba: [0, 0.5, 0.5, 1 ] 
        Rectangle:
            size: self.size
            pos: self.pos
    color: [1, 1, 1, 1]
    font_size: sp(self.width  * 0.1)
    size_hint_y: None
